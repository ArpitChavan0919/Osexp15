#include <stdio.h>
#include <dirent.h>

int main() {
    DIR *d;
    struct dirent *dir;
    d = opendir(".");  // Open current directory
    if (d) {
        while ((dir = readdir(d)) != NULL) {
            printf("%s\n", dir->d_name);  // Print file/folder name
        }
        closedir(d);
    } else {
        printf("Could not open directory.\n");
    }
    return 0;
}
